[vagrant]
ubuntutrusty ansible_ssh_host=localhost ansible_ssh_port=2221
centos7 ansible_ssh_host=localhost ansible_ssh_port=2222
archlinux ansible_ssh_host=localhost ansible_ssh_port=2223

[vagrant:vars]
ansible_ssh_user=vagrant
ansible_ssh_password=vagrant
ansible_ssh_common_args='-o StrictHostKeyChecking=no'

